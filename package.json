    "@types/react": "^18.3.17",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.10.4",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "whatwg-fetch": "^3.6.20"
  }
